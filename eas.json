{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "prebuildCommand": "git rev-parse HEAD > commit-hash.txt"
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "autoIncrement": true,
      "prebuildCommand": "git rev-parse HEAD > commit-hash.txt"
    }
  },
  "submit": {
    "production": {}
  }
}
